@model MLMWebsite.Models.UserAssets

@{
    ViewData["Title"] = "Create";
}


@*<h4 class="float-left">Profile Photo</h4>*@
<hr />
<div class="row">
    <div class="col-md-4 offset-2">
        <form asp-action="Create" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly"  class="text-danger"></div>
            <div class="form-group">
               <strong><label style="font-size:medium" asp-for="ProfilePhoto" class="control-label"></label></strong> 
                <input type="file" name="formFile" class="form-control" />
                <span asp-validation-for="ProfilePhoto" class="text-danger"></span>
            </div>
           
            <div class="form-group">
                <button class="btn btn-dark">
                    <a asp-action="Index" style="color:white" asp-controller="UserAssets">Edit</a>
                    </button>
                    <input type="submit" value="Upload" class="btn btn-dark" />
</div>
            <div>
                
                    
                
            </div>
        </form>
    </div>
</div>

