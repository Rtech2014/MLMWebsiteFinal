@model IEnumerable<MLMWebsite.Models.AddressProof>

@{
    ViewData["Title"] = "Index";
}




<div class="container" >
    <h2>Index</h2>
    
    
        <p>
            <a asp-action="Create">Create New</a>
        </p>
    
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Address)
                </th>
                @*<th>
                        @Html.DisplayNameFor(model => model.ApplicationUser)
                    </th>*@
                <th></th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @{
                            string imagepngData = Convert.ToBase64String(item.Address);
                            string imagepngDataURL = string.Format("data:image/png;base64,{0}", imagepngData);
                        }
                        <img src="@imagepngDataURL" width="200" alt="image" />
                    </td>
                    @*<td>
                            @Html.DisplayFor(modelItem => item.ApplicationUser.Id)
                        </td>*@
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                        <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>

